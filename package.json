{
  "name": "coindashboard-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "chart.js": "^3.9.1",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "npm": "^6.14.17",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-loader-spinner": "^5.1.7-beta.1",
    "react-query": "^3.39.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env BROWSER=none concurrently \"npm run start\" \"npx cypress run\"",
    "test:browser": "cross-env BROWSER=none concurrently \"npm run start\" \"npx cypress open\"",
    "eject": "react-scripts eject",
    "prepare": "husky install ./.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "cypress": "^10.6.0",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.6",
    "graphql": "^16.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "msw": "^0.46.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "tsc-files": "^1.1.3"
  },
  "msw": {
    "workerDirectory": "./public"
  }
}
